# API Configuration (server-only — never prefix with NEXT_PUBLIC_)
API_BASE_URL=http://localhost:5164

# Auth0 Configuration (server-only — never prefix with NEXT_PUBLIC_)
# v4 SDK env vars: AUTH0_DOMAIN (tenant domain, e.g. example.us.auth0.com),
# APP_BASE_URL (your app URL), AUTH0_SECRET (32-byte hex-encoded cookie secret)
AUTH0_DOMAIN=
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=
AUTH0_SECRET=
APP_BASE_URL=http://localhost:3000

# OpenTelemetry Configuration (server-only — never prefix with NEXT_PUBLIC_)
OTEL_EXPORTER_OTLP_ENDPOINT=
OTEL_EXPORTER_OTLP_HEADERS=
OTEL_SERVICE_NAME=maaldo-com-web
OTEL_RESOURCE_ATTRIBUTES=deployment.environment=Development,service.namespace=maaldo-com
